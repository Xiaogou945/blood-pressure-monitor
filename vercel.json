{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "handler": "handler",
                "includeFiles": [
                    "*.py",
                    "**/*.py"
                ],
                "excludeFiles": [
                    "**/*.pyc",
                    "**/__pycache__/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "index.py",
        "PYTHON_VERSION": "3.9",
        "PYTHONPATH": ".",
        "DEBUG": "true"
    }
}
